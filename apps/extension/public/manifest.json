{
  "manifest_version": 3,
  "name": "Prax wallet",
  "version": "8.0.0",
  "description": "For use in interacting with the Penumbra blockchain",
  "key": "",
  "minimum_chrome_version": "119",
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "favicon/icon16.png",
    "32": "favicon/icon32.png",
    "48": "favicon/icon48.png",
    "128": "favicon/icon128.png"
  },
  "options_ui": {
    "page": "page.html",
    "open_in_tab": true
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["manifest.json", "providers.html"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": [
    "cookies",
    "declarativeNetRequestWithHostAccess",
    "storage",
    "unlimitedStorage",
    "offscreen"
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "announce",
        "enabled": true,
        "path": "provider-redirect.json"
      }
    ]
  },
  "host_permissions": ["https://penumbra.zone.invalid/*"],
  "externally_connectable": {
    "matches": ["https://*/*"]
  },
  "content_security_policy": {
    "extension_pages": "object-src 'self'; script-src 'self' 'wasm-unsafe-eval'"
  }
}
