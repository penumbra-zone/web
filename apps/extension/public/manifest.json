{
  "manifest_version": 3,
  "name": "Penumbra wallet",
  "version": "3.1.0",
  "description": "For use in interacting with the Penumbra blockchain",
  "key": "",
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["injected-connection-manager.js"],
      "run_at": "document_start",
      "all_frames": true
    },
    {
      "matches": ["<all_urls>"],
      "js": ["injected-penumbra-global.js"],
      "run_at": "document_start",
      "all_frames": true,
      "world": "MAIN"
    }
  ],
  "background": {
    "service_worker": "service-worker.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["install-detector.txt"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": ["storage", "unlimitedStorage", "offscreen"],
  "host_permissions": ["<all_urls>"],
  "content_security_policy": {
    "extension_pages": "object-src 'self'; script-src 'self' 'wasm-unsafe-eval'"
  }
}
