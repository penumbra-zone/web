{
  "name": "@penumbra-zone/getters",
  "version": "6.2.0",
  "license": "(MIT OR Apache-2.0)",
  "description": "Convenience getters for the deeply nested optionals of Penumbra's protobuf types",
  "type": "module",
  "engine": {
    "node": ">=22"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rfv dist package penumbra-zone-*.tgz",
    "lint": "eslint src",
    "prepack": "$npm_execpath run build",
    "test": "vitest run"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./*": "./src/*.ts"
  },
  "publishConfig": {
    "exports": {
      "./*": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      }
    }
  },
  "devDependencies": {
    "@penumbra-zone/bech32m": "workspace:*"
  },
  "peerDependencies": {
    "@buf/penumbra-zone_penumbra.bufbuild_es": "1.9.0-20240528180215-8fe1c79485f8.1",
    "@bufbuild/protobuf": "^1.10.0",
    "@penumbra-zone/protobuf": "workspace:*"
  }
}
