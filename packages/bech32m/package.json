{
  "name": "@penumbra-zone/bech32m",
  "version": "4.0.0",
  "license": "(MIT OR Apache-2.0)",
  "description": "Tools for manipulating Penumbra bech32m strings",
  "type": "module",
  "scripts": {
    "build": "tsc --build",
    "clean": "rm -rfv dist",
    "lint": "eslint src",
    "test": "vitest run"
  },
  "files": [
    "./dist"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*.ts"
  },
