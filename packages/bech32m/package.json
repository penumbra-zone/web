    "exports": {
      ".": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "./*": {
        "default": "./dist/*.js",
        "types": "./dist/*.d.ts"
      }
    }
  },
  "dependencies": {
    "bech32": "^2.0.0"
  },
  "devDependencies": {
    "@buf/penumbra-zone_penumbra.bufbuild_es": "1.9.0-20240528180215-8fe1c79485f8.1"
  },
  "peerDependencies": {
    "@buf/penumbra-zone_penumbra.bufbuild_es": "1.9.0-20240528180215-8fe1c79485f8.1"
  }
}
