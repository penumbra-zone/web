{
  "name": "@penumbra-zone/client",
  "version": "7.0.0",
  "license": "(MIT OR Apache-2.0)",
  "description": "Package for connecting to any Penumbra extension, including Prax.",
  "type": "module",
  "engine": {
    "node": ">=22"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "rm -rfv dist package penumbra-zone-*.tgz tsconfig.tsbuildinfo",
    "lint": "eslint src"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./prax": "./src/prax.ts"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "peerDependencies": {
    "@penumbra-zone/protobuf": "workspace:*",
    "@penumbra-zone/transport-dom": "workspace:*"
  }
}
