{
  "name": "@penumbra-zone/client",
  "version": "3.0.0",
  "description": "Package for connecting to any Penumbra extension, including Prax.",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "clean": "rm -rfv dist",
    "lint": "eslint \"**/*.ts*\""
  },
  "exports": {
    ".": "./src/prax.ts",
    "./src/global": "./src/global.ts",
    "./src/get-port": "./src/get-port.ts"
  },
  "publishConfig": {
    "files": [
      "dist"
    ],
    "exports": {
      ".": "./dist/prax.js",
      "./global": "./dist/global.js",
      "./get-port": "./dist/get-port.js"
    }
  },
  "dependencies": {
    "@penumbra-zone/transport-dom": "workspace:*",
    "@penumbra-zone/types": "workspace:*"
  },
  "devDependencies": {
    "@bufbuild/protobuf": "^1.8.0",
    "@connectrpc/connect": "^1.4.0"
  },
  "peerDependencies": {
    "@bufbuild/protobuf": "^1.8.0",
    "@connectrpc/connect": "^1.4.0"
  }
}
