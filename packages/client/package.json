{
  "name": "@penumbra-zone/client",
  "version": "6.1.0",
  "license": "(MIT OR Apache-2.0)",
  "description": "Package for connecting to any Penumbra extension, including Prax.",
  "type": "module",
  "engine": {
    "node": ">=22"
  },
  "scripts": {
    "build": "tsup --clean src/*.ts --dts --minify --keep-names --format esm",
    "clean": "rm -rfv dist package penumbra-zone-*.tgz",
    "lint": "eslint src",
    "prepack": "$npm_execpath run build"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./prax": "./src/prax.ts"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./prax": {
        "types": "./dist/prax.d.ts",
        "default": "./dist/prax.js"
      }
    }
  },
  "dependencies": {
    "@connectrpc/connect": "^1.4.0",
    "@penumbra-zone/transport-dom": "workspace:*"
  },
  "peerDependencies": {
    "@penumbra-zone/protobuf": "workspace:*"
  }
}
