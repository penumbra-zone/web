{
  "name": "@penumbra-zone/query",
  "version": "4.1.1",
  "license": "(MIT OR Apache-2.0)",
  "type": "module",
  "engine": {
    "node": ">=22"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "rm -rfv dist package penumbra-zone-*.tgz tsconfig.tsbuildinfo",
    "lint": "eslint src",
    "test": "vitest run"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*"
