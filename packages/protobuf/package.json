{
  "name": "@penumbra-zone/protobuf",
  "version": "4.2.0",
  "license": "(MIT OR Apache-2.0)",
  "description": "Exports a `@bufbuild/protobuf` type registry with all message types necessary to communicate with a Penumbra extension",
  "type": "module",
  "engine": {
    "node": ">=22"
  },
  "scripts": {
    "build": "tsup --clean src/*.ts --dts --minify --keep-names --format esm",
    "clean": "rm -rfv dist package penumbra-zone-*.tgz",
    "lint": "eslint src",
    "prepack": "$npm_execpath run build"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./*": {
        "types": "./dist/*",
        "default": "./dist/*"
      },
      "./chunk-*": null
    }
  },
  "devDependencies": {
    "@bufbuild/protobuf": "^1.10.0"
  },
  "peerDependencies": {
    "@buf/cosmos_ibc.bufbuild_es": "1.9.0-20240530142100-ad4444393387.1",
    "@buf/cosmos_ibc.connectrpc_es": "1.4.0-20240530142100-ad4444393387.2",
    "@buf/penumbra-zone_penumbra.bufbuild_es": "1.9.0-20240528180215-8fe1c79485f8.1",
    "@buf/penumbra-zone_penumbra.connectrpc_es": "1.4.0-20240528180215-8fe1c79485f8.2",
    "@bufbuild/protobuf": "^1.10.0"
  }
}
