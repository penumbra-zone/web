{
  "name": "@penumbra-zone/transport-dom",
  "version": "6.0.0",
  "license": "(MIT OR Apache-2.0)",
  "type": "module",
  "engine": {
    "node": ">=22"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rfv dist package penumbra-zone-transport-dom-*.tgz",
    "lint": "eslint src",
    "prepack": "$npm_execpath run build",
    "test": "vitest run"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./*": "./src/*.ts"
  },
  "publishConfig": {
    "exports": {
      "./*": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      }
    }
  },
  "peerDependencies": {
    "@bufbuild/protobuf": "^1.10.0",
    "@connectrpc/connect": "^1.4.0"
  }
}
