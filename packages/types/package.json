{
  "name": "@penumbra-zone/types",
  "version": "7.1.1",
  "license": "(MIT OR Apache-2.0)",
  "type": "module",
  "engine": {
    "node": ">=22"
  },
  "scripts": {
    "build": "tsup --clean --entry 'src/*.ts' --entry '!src/*.test.ts' --dts --minify --keep-names --format esm",
    "clean": "rm -rfv dist package penumbra-zone-*.tgz",
    "lint": "eslint src",
    "prepack": "$npm_execpath run build",
    "test": "vitest run"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./*": "./src/*.ts"
  },
  "publishConfig": {
    "exports": {
      "./*": {
        "default": "./dist/*.js",
        "types": "./dist/*.d.ts"
      },
      "./chunk-*": null
    }
  },
  "dependencies": {
    "@penumbra-zone/bech32m": "workspace:*",
    "@penumbra-zone/getters": "workspace:*",
    "bignumber.js": "^9.1.2",
    "idb": "^8.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@buf/cosmos_ibc.bufbuild_es": "1.9.0-20240530142100-ad4444393387.1",
    "@buf/penumbra-zone_penumbra.bufbuild_es": "1.9.0-20240528180215-8fe1c79485f8.1",
    "@bufbuild/protobuf": "^1.10.0"
  },
  "peerDependencies": {
    "@buf/cosmos_ibc.bufbuild_es": "1.9.0-20240530142100-ad4444393387.1",
    "@buf/penumbra-zone_penumbra.bufbuild_es": "1.9.0-20240528180215-8fe1c79485f8.1",
    "@bufbuild/protobuf": "^1.10.0"
  }
}
