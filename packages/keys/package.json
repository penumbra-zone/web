{
  "name": "@penumbra-zone/keys",
  "version": "0.71.0",
  "description": "Proving keys for Penumbra",
  "type": "module",
  "scripts": {
    "clean": "rm -rfv ./dist ./keys",
    "prebuild": "./download-keys ./keys",
    "build": "tsc"
  },
  "exports": {
    ".": "./src/index.ts",
    "./*_pk": "./keys/*_pk.ts"
  },
  "publishConfig": {
    ".": {
      "import": "./index.js",
      "types": "./index.d.ts"
    },
    "./*_pk": {
      "import": "./dist/*_pk.js",
      "types": "./dist/*_pk.d.ts"
    }
  },
  "bin": {
    "download-keys": "./download-keys"
  }
}
