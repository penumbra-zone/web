{
  "name": "penumbra-web",
  "version": "1.0.0",
  "packageManager": "pnpm@8.6.11",
  "scripts": {
    "build": "turbo run build",
    "compile": "turbo run compile",
    "dev": "turbo run dev  --concurrency 20",
    "lint": "turbo run lint -- --max-warnings=0",
    "lint:fix": "turbo run lint -- --fix",
    "test": "turbo run test",
    "test:browser": "turbo run test:browser",
    "clean": "turbo clean",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "playwright-install": "playwright install",
    "all-check": "pnpm install && pnpm format-check && pnpm lint && pnpm test && pnpm test:browser && pnpm build"
  },
  "dependencies": {
    "@buf/cosmos_ibc.bufbuild_es": "1.7.2-20240215124455-b32ecf3ebbcb.1",
    "@buf/cosmos_ibc.connectrpc_es": "1.3.0-20240215124455-b32ecf3ebbcb.1",
    "@buf/penumbra-zone_penumbra.bufbuild_es": "1.7.2-20240216020833-dd8ae3a6cd04.1",
    "@buf/penumbra-zone_penumbra.connectrpc_es": "1.3.0-20240216020833-dd8ae3a6cd04.1",
    "@buf/tendermint_tendermint.bufbuild_es": "1.7.2-20231117195010-33ed361a9051.1",
    "@bufbuild/protobuf": "^1.7.2",
    "@connectrpc/connect": "^1.3.0",
    "@connectrpc/connect-web": "^1.3.0",
    "@penumbra-zone/tailwind-config": "workspace:*"
  },
  "devDependencies": {
    "@penumbra-zone/types": "workspace:*",
    "@penumbra-zone/wasm": "workspace:*",
    "@turbo/gen": "^1.12.4",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-custom": "workspace:*",
    "playwright": "^1.41.2",
    "prettier": "^3.2.5",
    "tsconfig": "workspace:*",
    "turbo": "^1.12.4",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  }
}
